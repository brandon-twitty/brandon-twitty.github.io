          "_name": "features1",
          "_customHTML": "<section class=\"features1 mbr-section\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"6\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"6\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Show Image\" name=\"showImage\" checked>\n        <input type=\"checkbox\" title=\"Show Main Title\" name=\"showmainTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Main Sub Title\" name=\"showmainsubTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showButtons\" checked>\n\n        <select title=\"Cards\" name=\"cardsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n        </select>\n        <header>Colors</header>\n        <input type=\"color\" title=\"Card Color\" name=\"cardbg\" value=\"#ffffff\" selected>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"container\">\n        <div class=\"mbr-row mbr-jc-c\" mbr-if=\"showmainTitle || showmainsubTitle\">\n            <div class=\"mbr-col-lg-6 mbr-pb-5 mbr-col-md-10 mbr-col-sm-12\">\n                <h3 class=\"main-subtitle mbr-fonts-style mbr-pb-2 align-center mbr-bold\" mbr-theme-style=\"display-4\" mbr-if=\"showmainTitle\" data-app-selector=\".main-subtitle\">GET IT DONE RIGHT</h3>\n                <h2 class=\"main-title mbr-fonts-style mbr-pb-2 align-center mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showmainsubTitle\" data-app-selector=\".main-title\"><span style=\"font-weight: normal;\">We</span> Install <span style=\"font-weight: normal;\">and</span> Repair Commercial <span style=\"font-weight: normal;\">and</span> Residential Roofs</h2>\n            </div>\n        </div>\n        <div class=\"mbr-row mbr-jc-c\">\n            <div class=\"card md-pb mbr-col-sm-12 mbr-col-md-6\" mbr-class=\"{'mbr-col-lg-4': cardsAmount == 3,\n                             'mbr-col-lg-3': cardsAmount == 4,\n                            'mbr-col-lg-2': cardsAmount == 5,\n                             'last-child': cardsAmount == 1}\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-img\" mbr-if=\"showImage\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/residentail-roof2-632x421.jpg\" layout=\"responsive\" width=\"316.750593824228\" height=\"211\" alt=\"ARS Constructions\" class=\"mobirise-loader\" title=\"house pic\">\n                            <div placeholder class=\"placeholder\"></div>\n                            <a href=\"https://app.gethearth.com/financing/15237/19452/prequalify?utm_campaign=15237&utm_content=general&utm_medium=contractor-website&utm_source=contractor&utm_term=19452\"><img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/residentail-roof2-632x421.jpg\" alt=\"ARS Constructions\" title=\"house pic\"></a>\n                        </amp-img>\n                    </div>\n                    <div class=\"card-box\" mbr-if=\"showTitle || showText || showButtons\">\n                        <h4 class=\"card-title mbr-fonts-style mbr-pb-2 align-left mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">Free Estimate</h4>\n                        <p class=\"mbr-text mbr-fonts-style mbr-semibold align-left\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\">Count on us for a hassle free estimate or inspection that meets your needs &amp; your budget.<br></p>\n\n                    </div>\n                    <div class=\"mbr-section-btn align-left\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\" mbr-buttons mbr-theme-style=\"display-4\"><a class=\"btn btn-md btn-danger\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\"><span class=\"mbri-right mbr-iconfont mbr-iconfont-btn\"></span>Book Now</a></div>\n                </div>\n            </div>\n            <div class=\"card md-pb mbr-col-sm-12 mbr-col-md-6\" mbr-class=\"{'mbr-col-lg-4': cardsAmount == 3,\n                             'mbr-col-lg-3': cardsAmount == 4,\n                            'mbr-col-lg-2': cardsAmount == 5,\n                             'last-child': cardsAmount == 2}\" mbr-if=\"cardsAmount > 1\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-img\" mbr-if=\"showImage\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/working-dudes-500x333.png\" layout=\"responsive\" width=\"315.3153153153153\" height=\"210\" alt=\"St Louis 24 hour Service\" class=\"mobirise-loader\" title=\"St Louis 24 hour Service\">\n                            <div placeholder class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/working-dudes-500x333.png\" alt=\"St Louis 24 hour Service\" title=\"St Louis 24 hour Service\">\n                        </amp-img>\n                    </div>\n                    <div class=\"card-box\" mbr-if=\"showTitle || showText || showButtons\">\n                        <h4 class=\"card-title mbr-fonts-style mbr-pb-2 align-left mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">24-Hour Emergency Service</h4>\n       